[root@SJK1 ~]#pvcreate pvname
[root@SJK1 ~]# vgcreate vgname pvname
[root@SJK1 ~]# lvcreate -L lvsize -n lvname  vgname
[root@SJK1 ~]# mkfs -t ext4 lvname
[root@SJK1 ~]# mount lvname  mount point


